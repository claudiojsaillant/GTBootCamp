<h1>Wishes</h1>

<h3>Save your wishes here!</h3>

<ul>
  {{#each wish}}
  	<li>
  	   {{!-- <p>ID: {{this.id}}</p> --}}
       <p>Wish: {{this.wish}}</p>
  	</li>
  {{/each}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="task"></textarea>

	<button type="submit">Submit</button>
</form>
